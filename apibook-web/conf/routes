# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                     controllers.Application.stackoverflow

GET        /apibook                     controllers.Application.index
GET        /stackoverflow                     controllers.Application.stackoverflow



POST    /api/search     controllers.Search.post

GET    /api/stackoverflow/summaries controllers.StackOverflow.summaries

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file         controllers.Assets.at(path="/public", file)

GET        /webjars/*file        controllers.WebJarAssets.at(file)

GET        /doc/*file           controllers.Assets.at(path="/public/doc", file)
